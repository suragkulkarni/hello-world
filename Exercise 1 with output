#include <stdio.h>
 
int main ()
{
   int n[100];
   int i;
   
   for(i=0;i<100;i++)
   {
       n[i] = rand();
   }
   
   int *p = n;
      for(i=0;i<100;i++)
          {
            printf("%d \t %d \n", *p+5, n[i]);
          }
          p++;
 }           
 
 
